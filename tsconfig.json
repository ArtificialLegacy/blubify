{
    "compilerOptions": {
        "strict": true,
        "target": "ES2022",
        "module": "CommonJS",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "incremental": false,
        "resolveJsonModule": true,
        "types": ["node"],
    }
}
